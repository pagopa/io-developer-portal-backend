{
  "name": "digital-citizenship-portal",
  "version": "0.5.0",
  "main": "./build/src/app",
  "typings": "src/typings",
  "license": "MIT",
  "engines": {
    "node": "10.14.1",
    "npm": "*"
  },
  "scripts": {
    "start": "node build/src/app",
    "prepare": "npm run snyk-protect && patch-package",
    "build:clean": "rimraf build",
    "build:tsc": "tsc",
    "build": "npm-run-all -s generate:api-models build:clean build:tsc",
    "clean:api-models": "rimraf generated/api && shx mkdir -p generated/api",
    "generate:api-models:public": "gen-api-models --api-spec https://raw.githubusercontent.com/pagopa/io-functions/v0.64.0/api/public_api_v1.yaml --no-strict --out-dir generated/api",
    "generate:api-models:admin": "gen-api-models --api-spec https://raw.githubusercontent.com/pagopa/io-functions-admin/master/openapi/index.yaml --no-strict --out-dir generated/api",
    "generate:api-models": "npm-run-all -s clean:api-models generate:api-models:*",
    "dist:modules": "modclean -r -n default:safe && npm prune --production",
    "predeploy": "npm-run-all build dist:modules",
    "snyk-protect": "snyk protect"
  },
  "dependencies": {
    "@types/memoizee": "^0.4.2",
    "applicationinsights": "^1.0.3",
    "azure-arm-apimanagement": "^2.1.1-preview",
    "azure-graph": "^3.1.0",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.2",
    "fp-ts": "^1.8.1",
    "helmet": "^3.21.1",
    "helmet-csp": "^2.9.3",
    "io-ts": "^1.3.0",
    "italia-ts-commons": "^2.14.0",
    "json-set-map": "^1.0.2",
    "memoizee": "^0.4.14",
    "method-override": "^2.3.10",
    "morgan": "^1.9.0",
    "ms-rest": "^2.2.4",
    "ms-rest-azure": "^2.4.4",
    "node-fetch": "^2.2.0",
    "passport": "*",
    "passport-azure-ad": "^3.0.8",
    "patch-package": "^5.1.1",
    "randomstring": "^1.1.5",
    "referrer-policy": "^1.1.0",
    "request": "^2.83.0",
    "ulid": "^2.3.0",
    "winston": "^3.0.0",
    "snyk": "^1.316.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.1",
    "@types/cookie-session": "^2.0.34",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^4.0.2",
    "@types/express": "^4.0.39",
    "@types/helmet": "^0.0.38",
    "@types/method-override": "^0.0.31",
    "@types/morgan": "^1.7.35",
    "@types/node-fetch": "^2.1.2",
    "@types/passport": "^0.4.5",
    "@types/randomstring": "^1.1.6",
    "@types/request": "^2.0.7",
    "@types/winston": "^2.3.7",
    "danger": "^4.0.2",
    "danger-plugin-digitalcitizenship": "^0.3.1",
    "italia-utils": "^4.1.0",
    "modclean": "^3.0.0-beta.1",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.7.4",
    "rimraf": "^2.6.3",
    "shx": "^0.3.2",
    "ts-node": "^6.1.1",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.6.0",
    "tslint-immutable": "^4.4.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.0.1"
  },
  "snyk": true
}
